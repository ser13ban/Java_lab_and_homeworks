<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
  <persistence-unit name="default">

    <class>Entityes.GenresEntity</class>
    <class>Entityes.GuideEntity</class>
    <class>Entityes.MoviesEntity</class>
    <properties>
      <property name="eclipselink.jdbc.url" value="jdbc:mariadb://localhost:3306/movies"/>
      <property name="eclipselink.jdbc.driver" value="org.mariadb.jdbc.Driver"/>
    </properties>
  </persistence-unit>
  <persistence-unit name="persistenceUnit"/>
  <persistence-unit name="EclipseLink"/>
</persistence>
